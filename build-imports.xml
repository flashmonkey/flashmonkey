<!-- Assets imported by this project -->
<project name="flashmonkey.imports">
	<!--
          ant.file.[projectname] is a built-in property that contains
          the current file's full path. Each file must have a name
          and each name must be unique for a project.
    -->
    <dirname property="flashmonkey.imports.dir" file="${ant.file.flashmonkey.imports}"/>
	
	<property file="${flashmonkey.imports.dir}/build-user.properties" />
	
	<import file="build-flashmonkey-properties.xml"/>
    <import file="build-flashmonkey-targets.xml"/>
    <import file="../antennae/tools/build-common-imports.xml" />

    <import file="build-assets.xml" />
</project>
