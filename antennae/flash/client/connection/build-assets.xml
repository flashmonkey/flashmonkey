<!-- Build assets for library1.  Note that all paths are defined
     relative to ${multi.dir} since we don't know what the current
     directory is in which Ant is running. -->
<project name="paperworld-connection.assets">
    <dirname property="paperworld-connection.assets.dir" file="${ant.file.paperworld-connection.assets}"/>

    <!-- Define SWC exported by this library. -->
    <property name="paperworld.paperworld-connection.swc" location="${paperworld-connection.assets.dir}/${build.compc.dir}/paperworld-connection.swc"/>

</project>
