<!-- Build assets for library1.  Note that all paths are defined
     relative to ${multi.dir} since we don't know what the current
     directory is in which Ant is running. -->
<project name="HelloPaperworldFlash.assets">
    <dirname property="HelloPaperworldFlash.assets.dir" file="${ant.file.HelloPaperworldFlash.assets}"/>

    <!-- Define SWC exported by this library. -->
    <property name="HelloPaperworldFlash.swf" location="${HelloPaperworldFlash.assets.dir}/${build.mxmlc.dir}/HelloPaperworldFlash.swf"/>

</project>
