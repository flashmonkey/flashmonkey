<!-- Build assets for library1.  Note that all paths are defined
     relative to ${multi.dir} since we don't know what the current
     directory is in which Ant is running. -->
<project name="paperworld-java-core.assets">
    <dirname property="paperworld-java-core.assets.dir" file="${ant.file.paperworld-java-core.assets}"/>
	
	<property name="eventdispatcher.jar" location="${paperworld-java-core.assets.dir}/../lib/EventDispatcher-0.12.0.jar" />

    <!-- Define JAR exported by this library. -->
    <property name="paperworld-java-core.jar" location="${paperworld-java-core.assets.dir}/${build.dist.dir}/paperworld-java-core.jar"/>

</project>
