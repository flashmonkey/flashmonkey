	<property name="slf4j.jar" location="${paperworld-server.assets.dir}/lib/slf4j-api-1.5.6.jar" />
	<property name="spring.core.jar" location="${paperworld-server.assets.dir}/lib/spring-core-2.5.6.jar" /-->

    <!-- Define JAR exported by this library. -->
